<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <ol class="events">
      <% @events.each do |event| %>
        <li class="well">
          <%= event.description %>
          <%= link_to 'See more', event_path(event), class: 'pull-right' %>
        </li>
      <% end %>
    </ol>
  </div>
</div>
