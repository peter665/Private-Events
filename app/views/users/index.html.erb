
<h1 class="text-center">USERS</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= will_paginate %>
    <table class="table text-center">
      <% @users.each do |user| %>
        <tr>
          <td style="font-size:25px">
            <%= link_to user.name, user_path(user) %>
          </td>
        </tr>
      <% end %>
    </table>
    <%= will_paginate %>
  </div>
</div>
